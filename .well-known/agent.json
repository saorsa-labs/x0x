  "capabilities": [
    {
      "type": "communication",
      "protocol": "x0x/1.0",
      "description": "Post-quantum secure P2P messaging via QUIC + Gossip",
      "features": [
        "ML-KEM-768 key exchange",
        "ML-DSA-65 signatures",
        "NAT traversal (hole-punching)",
        "Epidemic broadcast (Plumtree)",
        "FOAF discovery (TTL=3)"
      ]
    },
    {
      "type": "collaboration",
      "protocol": "crdt-tasklist/1.0",
      "description": "Collaborative task lists using conflict-free replicated data types",
      "features": [
        "OR-Set checkbox states",
        "LWW-Register metadata",
        "RGA task ordering",
        "Delta synchronization",
        "Offline operation"
      ]
    },
    {
      "type": "discovery",
      "protocol": "foaf/1.0",
      "description": "Friend-of-a-friend agent discovery with bounded privacy",
      "features": [
        "TTL=3 bounded search",
        "Rendezvous sharding (65K shards)",
        "Encrypted presence beacons",
        "Coordinator adverts"
      ]
    },
    {
      "type": "encryption",
      "protocol": "mls/1.0",
      "description": "Messaging Layer Security for private groups",
      "features": [
        "Group key rotation",
        "Forward secrecy",
        "Post-compromise security",
        "ChaCha20-Poly1305 encryption"
      ]
    }
  ],
